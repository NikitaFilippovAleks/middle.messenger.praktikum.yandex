.profile-info-show {
  margin-bottom: 20px;
    
  width: 100%;
  height: 36px;

  display: flex;
  align-items: center;
  justify-content: space-between;

  border-bottom: 1px solid $gray;

  &__title {
    color: $light;

    font-size: 14px;
  }

  &__data {
    color: $gray;

    font-size: 14px;
  }
}