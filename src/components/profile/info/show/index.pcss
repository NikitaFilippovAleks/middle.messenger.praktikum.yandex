.profile-info-show {
  display: flex;

  align-items: center;
  justify-content: space-between;

  width: 100%;
  height: 36px;
  margin-bottom: 20px;

  border-bottom: 1px solid $gray;

  &__title {
    font-size: 14px;
    color: $light;
  }

  &__data {
    font-size: 14px;
    color: $gray;
  }
}
